{
  "$schema": "https://statics.teams.microsoft.com/sdk/v1.0/manifest/MicrosoftTeams.schema.json",
  "manifestVersion": "1.0",
  "version": "1.0",
  "id": "c0ec0b3c-acc4-417a-8b59-51abb0ef5476",
  "packageName": "com.microstack.microsoftteams",
  "developer": {
    "name": "MicroStack",
    "websiteUrl": "https://stackoverflow.microsoft.com/",
    "privacyUrl": "https://stackoverflow.microsoft.com/",
    "termsOfUseUrl": "https://stackoverflow.microsoft.com/"
  },
  "name": {
    "short": "MicroStack",
    "full": "MicroStack - Microsoft Stack Overflow"
  },
  "description": {
    "short": "MicroStack - a bot for tracking Microsoft Stack Overflow",
    "full": "MicroStack - a bot for tracking Microsoft Stack Overflow"
  },
  "icons": {
    "outline": "stack-overflow.png",
    "color": "stack-overflow.png"
  },
  "accentColor": "#F48024",
  "bots": [
    {
      "botId": "c0ec0b3c-acc4-417a-8b59-51abb0ef5476",
      "scopes": [
        "team",
        "personal"
      ],
      "commandLists": [
        {
          "scopes": [
            "team"
          ],
          "commands": [
            {
              "title": "follow tags",
              "description": "Get notified on all questions with specific tags"
            },
            {
              "title": "remove tags",
              "description": "Stop notifications for specific tags"
            },
            {
              "title": "settings",
              "description": "View, add, and remove the tags you are following"
            }
          ]
        },
        {
          "scopes": [
            "personal"
          ],
          "commands": [
            {
              "title": "follow tags",
              "description": "Get notified on all questions with specific tags"
            },
            {
              "title": "remove tags",
              "description": "Stop notifications for specific tags"
            },
            {
              "title": "settings",
              "description": "View, add, and remove the tags you are following"
            }
          ]
        }
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "microstack-test.azurewebsites.net"
  ]
}